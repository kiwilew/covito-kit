<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="120"
		timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120" />

	<cache name="SYS_REL_CACHE" maxElementsInMemory="10000" eternal="true" overflowToDisk="true" />

	<cache name="role" maxElementsInMemory="10000" eternal="false" overflowToDisk="true"
		timeToIdleSeconds="120" timeToLiveSeconds="120" />

	<cache name="user" maxElementsInMemory="10000" eternal="false" overflowToDisk="true"
		timeToIdleSeconds="120" timeToLiveSeconds="120" />

</ehcache>